import {Component} from "@angular/core";

@Component({
	templateUrl:'partials/cal.component.html',
	selector:'cool-app',
})
export class CalComponent {
	
	title:string = "Simple Math Calculator";
	valOne:number = 10;
	valTwo:number = 10;
	result:number = 20;
	
	doSum(){
		this.result = this.valOne + this.valTwo;			
	}

	doDiff(){
		this.result = this.valOne - this.valTwo;			
	}

}
